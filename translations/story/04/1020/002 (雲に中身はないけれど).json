            "blockIdx": 27
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（ガコン！！）",
            "enText": "(Thunk!!)",
            "nextBlock": 29,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 35,
                    "pathId": -3148952081016697055
                },
                {
                    "origLen": 19,
