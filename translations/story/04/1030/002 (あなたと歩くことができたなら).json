            "pathId": -639558383286818676,
            "blockIdx": 5
        },
        {
            "jpName": "ライスシャワー",
            "enName": "Rice Shower",
            "jpText": "…………ううんっ、でも、がんばらなきゃ！",
            "enText": "But even still I have to do my best!",
            "nextBlock": 7,
            "origClipLength": 119,
            "animData": [
                {
                    "origLen": 53,
                    "pathId": -2015516918266141748
                },
                {
                    "origLen": 53,
                    "pathId": 4086271452035677023
