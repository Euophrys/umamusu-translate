            "animData": [
                {
                    "origLen": 120,
                    "pathId": -1701790776745288376
                },
                {
                    "origLen": 91,
                    "pathId": -992009698613757015
                },
                {
                    "origLen": 233,
                    "pathId": 306954968908406626
                }
            ],
            "pathId": -3017345493155933841,
            "blockIdx": 22
        },
        {
            "jpName": "エアグルーヴ",
            "enName": "Air Groove",
            "jpText": "厳重注意だ。",
            "enText": "I'm here to give you a warning.",
            "nextBlock": 24,
            "origClipLength": 46,
            "animData": [
                {
                    "origLen": 60,
                    "pathId": -5098881806042971754
                },
