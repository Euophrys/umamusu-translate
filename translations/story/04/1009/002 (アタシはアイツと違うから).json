            "blockIdx": 19
        },
        {
            "jpName": "ダイワスカーレット",
            "enName": "Daiwa Scarlet",
            "jpText": "…………っ。",
            "enText": "…",
            "nextBlock": 21,
            "origClipLength": 38,
            "animData": [
                {
                    "origLen": 62,
                    "pathId": -7702859648692326236
