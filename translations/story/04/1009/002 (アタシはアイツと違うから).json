            "blockIdx": 20
        },
        {
            "jpName": "エアグルーヴ",
            "enName": "Air Groove",
            "jpText": "……ここにいたか、ダイワスカーレット。",
            "enText": "There you are, Daiwa Scarlet.",
            "nextBlock": 22,
            "origClipLength": 89,
            "animData": [
                {
                    "origLen": 101,
                    "pathId": -5061983452037008862
                },
                {
                    "origLen": 101,
                    "pathId": -8250313137424447544
