            "blockIdx": 21
        },
        {
            "jpName": "トウカイテイオー",
            "enName": "",
            "jpText": "はっ、はっ、はっ！\nよーし、今日も絶好調！",
            "enText": "",
            "nextBlock": 23,
            "origClipLength": 161,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 1517191507541566172
                },
                {
                    "origLen": 217,
                    "pathId": -5040703025972994661
                }
