            "pathId": -2500493759387185903,
            "blockIdx": 8
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "…………えっ。",
            "enText": "…Eh?",
            "nextBlock": 10,
            "origClipLength": 24,
            "animData": [
                {
                    "origLen": 21,
                    "pathId": 5714482928908489018
                },
                {
