            "pathId": -1264731114078680021,
            "blockIdx": 15
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "つまんない映画……。",
            "enText": "A boring movie…",
            "nextBlock": 17,
            "origClipLength": 60,
            "animData": [
                {
                    "origLen": 148,
                    "pathId": -3093868065198243787
                }
