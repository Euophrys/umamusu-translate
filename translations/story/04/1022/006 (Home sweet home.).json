            "pathId": 7371906343900508173,
            "blockIdx": 30
        },
        {
            "jpName": "ファインモーション",
            "enName": "Fine Motion",
            "jpText": "ふふ……せ～のっ！",
            "enText": "Fufu… 1, 2-!",
            "nextBlock": 32,
            "origClipLength": 95,
            "animData": [
                {
                    "origLen": 60,
                    "pathId": -1495946690665310093
                },
