            "pathId": 232313546624597215,
            "blockIdx": 17
        },
        {
            "jpName": "ファインモーション",
            "enName": "Fine Motion",
            "jpText": "ゆめ……。",
            "enText": "Dreams…",
            "nextBlock": 19,
            "origClipLength": 45,
            "animData": [
                {
                    "origLen": 67,
                    "pathId": -7841939549475223258
                },
                {
                    "origLen": 59,
                    "pathId": -7390635496142733121
                }
            ],
            "pathId": 2675386887039305465,
            "blockIdx": 18
        },
        {
            "jpName": "ファインモーション",
            "enName": "Fine Motion",
            "jpText": "夢……私の……？\r\n…………………………。",
            "enText": "Dreams…mine…? \n…",
            "nextBlock": 20,
            "origClipLength": 239,
            "animData": [
                {
                    "origLen": 184,
                    "pathId": 8798453846089835521
