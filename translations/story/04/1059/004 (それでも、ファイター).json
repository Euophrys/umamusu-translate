            "nextBlock": 14,
            "origClipLength": 120,
            "choices": [
                {
                    "jpText": "いいタイム出たぞ、ドーベル",
                    "enText": "That was a good time, Dober",
                    "nextBlock": 14
                },
                {
                    "jpText": "いいタイム出たよ、ドーベル",
                    "enText": "That was a good time, Dober",
                    "nextBlock": 14
