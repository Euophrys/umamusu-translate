            "animData": [
                {
                    "origLen": 67,
                    "pathId": 4217726886622881938
                }
            ],
            "pathId": 5769597007397657132,
            "blockIdx": 3
        },
        {
            "jpName": "ワンダーアキュート",
            "enName": "",
            "jpText": "はぁ……はぁ……。うん、いい調子じゃねぇ。\nトレーナーさんの指示に、体がちゃんとついていけとるよお。",
            "enText": "",
            "nextBlock": 5,
            "origClipLength": 402,
            "animData": [
