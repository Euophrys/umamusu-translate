            "nextBlock": 27,
            "origClipLength": 115,
            "animData": [
                {
                    "origLen": 291,
                    "pathId": 155349114531337026
                }
            ],
            "pathId": -9083997962948250994,
            "blockIdx": 26
        },
        {
            "jpName": "ユキノビジン",
            "enName": "",
            "jpText": "……“シチーガール”は……。",
            "enText": "",
            "nextBlock": 28,
            "origClipLength": 146,
            "animData": [
