            "blockIdx": 26
        },
        {
            "jpName": "幼いライアン",
            "enName": "Young Ryan",
            "jpText": "やったぁーっ！　あたしがいちばんだー！！",
            "enText": "Yay! I'm the best!",
            "nextBlock": 28,
            "origClipLength": 106,
            "pathId": 4655989770867936033,
            "blockIdx": 27
        },
        {
            "jpName": "メジロライアン",
            "enName": "Mejiro Ryan",
            "jpText": "…………ッ！！",
            "enText": "…!!!",
            "nextBlock": 29,
            "origClipLength": 39,
            "animData": [
                {
                    "origLen": 134,
                    "pathId": 176445359492273287
                }
