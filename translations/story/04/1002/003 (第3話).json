            "blockIdx": 29
        },
        {
            "jpName": "サイレンススズカ",
            "enName": "Silence Suzuka",
            "jpText": "はっ、はっ、はっ……ふぅ。",
            "enText": "Haa, haa, haa… Fuuu.",
            "nextBlock": 31,
            "origClipLength": 191,
            "animData": [
                {
                    "origLen": 183,
                    "pathId": 5062923143533685397
                },
