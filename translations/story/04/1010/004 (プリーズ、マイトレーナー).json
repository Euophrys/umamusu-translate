            "pathId": -1472131890407352225,
            "blockIdx": 12
        },
        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "うぅ……グスッ……！！",
            "enText": "Uu… hicc…!!",
            "nextBlock": 14,
            "origClipLength": 66,
            "animData": [
                {
                    "origLen": 50,
                    "pathId": 8055481493722990292
                }
