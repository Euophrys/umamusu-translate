        },
        {
            "jpName": "タイキシャトル",
            "enName": "Taiki Shuttle",
            "jpText": "レッツゴー！　ゴーゴーゴーーッ！！",
            "enText": "Let's go! Go go goooo!!",
            "nextBlock": 30,
            "origClipLength": 128,
            "animData": [
