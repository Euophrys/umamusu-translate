            "blockIdx": 3
        },
        {
            "jpName": "アグネスタキオン",
            "enName": "Agnes Tachyon",
            "jpText": "ふぅン……。",
            "enText": "Hmmm…",
            "nextBlock": 5,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 11,
                    "pathId": 2958797085245021165
                },
                {
                    "origLen": 11,
                    "pathId": 956362238146610597
