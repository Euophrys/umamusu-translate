            "pathId": 5771692417987596857,
            "blockIdx": 12
        },
        {
            "jpName": "ツルマルツヨシ",
            "enName": "",
            "jpText": "……私、昔からこうなんです。",
            "enText": "",
            "nextBlock": 14,
            "origClipLength": 63,
            "animData": [
                {
                    "origLen": 26,
                    "pathId": -4292549431757380271
