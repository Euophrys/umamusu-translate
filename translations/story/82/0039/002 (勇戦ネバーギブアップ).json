            "blockIdx": 2
        },
        {
            "jpName": "ウオッカ",
            "enName": "Vodka",
            "jpText": "っし！　またまた連勝！",
            "enText": "Alright! Another consecutive win!",
            "nextBlock": 4,
            "origClipLength": 59,
            "animData": [
                {
                    "origLen": 43,
                    "pathId": 8325693199439245295
                },
