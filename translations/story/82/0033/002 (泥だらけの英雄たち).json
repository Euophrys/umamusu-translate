            "blockIdx": 10
        },
        {
            "jpName": "エルコンドルパサー",
            "enName": "",
            "jpText": "うわーっとととっ！？",
            "enText": "",
            "nextBlock": 12,
            "origClipLength": 58,
            "choices": [
                {
                    "jpText": "だ、大丈夫！？",
