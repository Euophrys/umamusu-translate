            "animData": [
                {
                    "origLen": 175,
                    "pathId": 3378287964087432924
                },
                {
                    "origLen": 156,
                    "pathId": -421856592500253287
                },
                {
                    "origLen": 99,
                    "pathId": 3757115899069449449
