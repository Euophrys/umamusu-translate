            "animData": [
                {
                    "origLen": 183,
                    "pathId": 5278981631224293318
                },
                {
                    "origLen": 130,
                    "pathId": 3882039654109466826
                },
                {
                    "origLen": 99,
                    "pathId": 7882327242648128603
                }
            ],
            "pathId": 2088477307468651823,
            "blockIdx": 16
        },
        {
            "jpName": "エルコンドルパサー",
            "enName": "",
            "jpText": "フフン……気になりますか？\r\nなら、ついてきてもいいデスよ！",
            "enText": "",
            "nextBlock": 18,
            "origClipLength": 84,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": -8192463649132112175
