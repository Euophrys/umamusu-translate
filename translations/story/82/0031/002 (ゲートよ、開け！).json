            "pathId": -447409763321026762,
            "blockIdx": 25
        },
        {
            "jpName": "シンコウウインディ",
            "enName": "Shinko Windy",
            "jpText": "うりゃああ～～～！！",
            "enText": "Uryaaaahhhhh!",
            "nextBlock": 27,
            "origClipLength": 84,
            "trainerGender": "f",
            "animData": [
                {
                    "origLen": 136,
                    "pathId": -4718659165862608459
