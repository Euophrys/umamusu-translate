            "jpText": "……ううっ……ううう……。",
            "enText": "… ugh… ugh…",
            "nextBlock": 33,
            "origClipLength": 63,
            "choices": [
                {
                    "jpText": "大丈夫か！？",
                    "enText": "Are you OK?",
                    "nextBlock": 33
                },
                {
                    "jpText": "大丈夫！？",
                    "enText": "A great man!",
                    "nextBlock": 33
