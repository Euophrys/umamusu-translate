            "pathId": -3101531875890655288,
            "blockIdx": 2
        },
        {
            "jpName": "ウマ娘A",
            "enName": "Horsegirl A",
            "jpText": "あ、姐さん……？",
            "enText": "Oh, Miss…?",
            "nextBlock": 4,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 161,
                    "pathId": 5895048043467486315
