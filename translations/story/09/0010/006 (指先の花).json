            "blockIdx": 2
        },
        {
            "jpName": "トーセンジョーダン",
            "enName": "Tosen Jordan",
            "jpText": "そ、ネイル～！",
            "enText": "Yeah, nails～!",
            "nextBlock": 4,
            "origClipLength": 72,
            "animData": [
                {
                    "origLen": 60,
