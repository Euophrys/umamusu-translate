            "pathId": -1952827526676275614,
            "blockIdx": 19
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "（バキャッ！！　ビキッ、ベリリ……ッ！！）",
            "enText": "(Bonk! Brunk, Brrrr…!!)",
            "nextBlock": 21,
            "origClipLength": 72,
            "animData": [
                {
                    "origLen": 106,
                    "pathId": -2220855417531074349
                }
