            "blockIdx": 1
        },
        {
            "jpName": "ワンダーアキュート",
            "enName": "",
            "jpText": "あ、ああ……トレーナーさん。",
            "enText": "",
            "nextBlock": 3,
            "origClipLength": 63,
            "choices": [
                {
                    "jpText": "すごい量の料理だけど……",
