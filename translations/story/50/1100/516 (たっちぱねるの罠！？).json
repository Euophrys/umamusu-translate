            ],
            "pathId": 8842990984587704101,
            "blockIdx": 3
        },
        {
            "jpName": "ワンダーアキュート",
            "enName": "",
            "jpText": "ようわからんうちに画面を触っとたら、\nこんなことになっとったんよ……。",
            "enText": "",
            "nextBlock": 5,
            "origClipLength": 90,
            "choices": [
                {
                    "jpText": "事情を説明したら？",
                    "enText": "",
                    "nextBlock": 5
