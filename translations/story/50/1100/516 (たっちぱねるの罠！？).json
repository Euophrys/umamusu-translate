                {
                    "origLen": 53,
                    "pathId": -3128585536261432225
                },
                {
                    "origLen": 101,
                    "pathId": 7424848914217323914
                }
            ],
            "pathId": -3813525024112746320,
            "blockIdx": 4
        },
        {
            "jpName": "ワンダーアキュート",
            "enName": "",
            "jpText": "いやいや、食べられんことはないんよ。",
            "enText": "",
            "nextBlock": 6,
            "origClipLength": 68,
            "animData": [
                {
                    "origLen": 59,
                    "pathId": -2375558668902493355
