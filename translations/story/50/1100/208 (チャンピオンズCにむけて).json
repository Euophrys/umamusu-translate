            "nextBlock": 24,
            "origClipLength": 56,
            "choices": [
                {
                    "jpText": "ああ、これこそが本物だ",
                    "enText": "",
                    "nextBlock": 24
                },
                {
                    "jpText": "うん、これこそ本物だと思う",
                    "enText": "",
                    "nextBlock": 24
