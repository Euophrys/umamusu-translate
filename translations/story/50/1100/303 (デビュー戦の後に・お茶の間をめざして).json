            ],
            "pathId": 4047865833230695349,
            "blockIdx": 10
        },
        {
            "jpName": "ワンダーアキュート",
            "enName": "",
            "jpText": "ほほぉ、そういうもの……。",
            "enText": "",
            "nextBlock": 12,
            "origClipLength": 88,
            "animData": [
                {
                    "origLen": 57,
                    "pathId": 1731377190969772020
