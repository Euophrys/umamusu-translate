        {
            "jpName": "ワンダーアキュート",
            "enName": "",
            "jpText": "だけど……。",
            "enText": "",
            "nextBlock": 11,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 27,
                    "pathId": -359433174885615059
