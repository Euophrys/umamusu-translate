            "pathId": -1323569966407460145,
            "blockIdx": 20
        },
        {
            "jpName": "ワンダーアキュート",
            "enName": "",
            "jpText": "そう……だからあんなに輝いて見えて……。",
            "enText": "",
            "nextBlock": 22,
            "origClipLength": 71,
            "choices": [
                {
                    "jpText": "君のキラキラも知ってもらおう！",
                    "enText": "",
                    "nextBlock": 23
                },
                {
                    "jpText": "貴方のキラキラも知ってもらおう！",
                    "enText": "",
                    "nextBlock": 23
                },
                {
                    "jpText": "もっと楽しんでる君が見たいな",
                    "enText": "",
                    "nextBlock": 27
