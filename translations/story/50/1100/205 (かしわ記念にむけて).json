            "pathId": -4014340948779137917,
            "blockIdx": 15
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（ワアアアアアアアアアアッ）",
            "enText": "",
            "nextBlock": 17,
            "origClipLength": 63,
            "animData": [
                {
                    "origLen": 13,
                    "pathId": 7750021022830365704
