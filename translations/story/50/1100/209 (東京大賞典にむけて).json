                {
                    "origLen": 172,
                    "pathId": -799742167348996809
                },
                {
                    "origLen": 152,
                    "pathId": 259715723531861362
                }
            ],
            "pathId": 5284373779289013690,
            "blockIdx": 13
        },
        {
            "jpName": "スマートファルコン",
            "enName": "",
            "jpText": "きっと、私がずーっと待ってたウマ娘は……。",
            "enText": "",
            "nextBlock": 15,
            "origClipLength": 72,
            "animData": [
                {
                    "origLen": 58,
