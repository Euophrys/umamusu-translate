            "enText": "",
            "nextBlock": 28,
            "origClipLength": 99,
            "choices": [
                {
                    "jpText": "落ち着いていて、すごいな！",
                    "enText": "",
                    "nextBlock": 28
                },
                {
                    "jpText": "落ち着いていて、すごいね！",
                    "enText": "",
