                }
            ],
            "pathId": -8678378166601184172,
            "blockIdx": 15
        },
        {
            "jpName": "ワンダーアキュート",
            "enName": "",
            "jpText": "ファル子さん……。",
            "enText": "",
            "nextBlock": 17,
            "origClipLength": 56,
            "animData": [
                {
                    "origLen": 31,
