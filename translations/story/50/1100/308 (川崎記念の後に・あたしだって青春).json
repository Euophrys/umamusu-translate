            "pathId": 2414636578186557033,
            "blockIdx": 10
        },
        {
            "jpName": "スマートファルコン",
            "enName": "",
            "jpText": "最っ高だったよ！！",
            "enText": "",
            "nextBlock": 12,
            "origClipLength": 56,
            "animData": [
                {
                    "origLen": 67,
                    "pathId": -8370809991751579715
