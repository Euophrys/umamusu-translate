            "pathId": -1710670007173389571,
            "blockIdx": 14
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "『<itemname>』を獲得した",
            "enText": "",
            "nextBlock": 16,
            "origClipLength": 67,
            "coloredText": [
                {
                    "jpText": "<itemname>",
                    "enText": ""
