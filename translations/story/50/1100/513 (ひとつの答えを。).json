            "animData": [
                {
                    "origLen": 76,
                    "pathId": 5854993572655388240
                },
                {
                    "origLen": 104,
                    "pathId": -9193209917819158462
                }
            ],
            "pathId": 1720914180756138084,
            "blockIdx": 25
        },
        {
            "jpName": "ワンダーアキュート",
            "enName": "",
            "jpText": "むしろだからこそ、かねぇ。",
            "enText": "",
            "nextBlock": 27,
            "origClipLength": 61,
            "animData": [
                {
                    "origLen": 40,
                    "pathId": 5284670817330194725
