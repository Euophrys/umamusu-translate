            "nextBlock": 21,
            "origClipLength": 81,
            "choices": [
                {
                    "jpText": "ほかほかまんじゅう",
                    "enText": "",
                    "nextBlock": 21
                },
                {
                    "jpText": "幸運の杖",
                    "enText": "",
                    "nextBlock": 30
                },
                {
