            "pathId": 2434045608406936933,
            "blockIdx": 6
        },
        {
            "jpName": "ワンダーアキュート",
            "enName": "",
            "jpText": "しばらくゆっくり休んでいいのかい？",
            "enText": "",
            "nextBlock": 8,
            "origClipLength": 67,
            "choices": [
                {
                    "jpText": "もちろん！",
                    "enText": "",
                    "nextBlock": 8
                }
