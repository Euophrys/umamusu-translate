            "pathId": 7690467705397596075,
            "blockIdx": 7
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "――変わった。明らかに。",
            "enText": "",
            "nextBlock": 9,
            "origClipLength": 60,
            "pathId": 4427038311636256542,
            "blockIdx": 8
        },
        {
            "jpName": "ワンダーアキュート",
            "enName": "",
            "jpText": "はぁ、はぁ、はぁ……。",
            "enText": "",
            "nextBlock": 10,
            "origClipLength": 151,
            "animData": [
