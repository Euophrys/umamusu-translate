            "pathId": -8865853422202598372,
            "blockIdx": 2
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "――変わった。",
            "enText": "",
            "nextBlock": 4,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 65,
                    "pathId": -7266853309286036725
