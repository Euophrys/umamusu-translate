                {
                    "origLen": 101,
                    "pathId": -7348827588066640150
                }
            ],
            "pathId": -586327859889841781,
            "blockIdx": 9
        },
        {
            "jpName": "ワンダーアキュート",
            "enName": "",
            "jpText": "構いませんよぉ、ありがとうございます。",
            "enText": "",
            "nextBlock": 11,
            "origClipLength": 98,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 8123739605783550729
                },
                {
                    "origLen": 61,
                    "pathId": 2188617308218333126
