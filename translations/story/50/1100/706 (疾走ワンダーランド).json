            "pathId": 7533427819506999503,
            "blockIdx": 13
        },
        {
            "jpName": "ワンダーアキュート",
            "enName": "",
            "jpText": "あらまあ、ジェットコースターかい……？",
            "enText": "",
            "nextBlock": 15,
            "origClipLength": 69,
            "animData": [
                {
                    "origLen": 72,
                    "pathId": 8141522015072177054
                },
