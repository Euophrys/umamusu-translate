            "pathId": 3390804581126303782,
            "blockIdx": 22
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（ブオンッ！　ブオンッ！）",
            "enText": "",
            "nextBlock": 24,
            "origClipLength": 61,
            "animData": [
                {
                    "origLen": 13,
                    "pathId": -3135334747859441380
