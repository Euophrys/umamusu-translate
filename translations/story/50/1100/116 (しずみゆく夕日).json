            "animData": [
                {
                    "origLen": 51,
                    "pathId": 7030860887154900228
                },
                {
                    "origLen": 100,
                    "pathId": -7688052376791894323
                }
            ],
            "pathId": -4100636679403059718,
            "blockIdx": 5
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "おかしくは……なかった。むしろ当然だった。\n生気が枯れているのは、トレーナーである\n自分には、よくわかっている。",
            "enText": "",
            "nextBlock": 7,
            "origClipLength": 118,
            "choices": [
                {
                    "jpText": "とにかく焦らずにいこう",
                    "enText": "",
                    "nextBlock": 7
                }
            ],
            "animData": [
                {
                    "origLen": 129,
                    "pathId": 5215903712231715811
                },
                {
                    "origLen": 129,
                    "pathId": 7413909939072350772
                }
