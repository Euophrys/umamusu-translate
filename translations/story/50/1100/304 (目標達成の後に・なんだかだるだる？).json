            "nextBlock": 3,
            "origClipLength": 80,
            "animData": [
                {
                    "origLen": 160,
                    "pathId": 2001325986417588830
                },
                {
                    "origLen": 162,
                    "pathId": -5752053686486001660
                }
            ],
            "pathId": 3840988567044473803,
            "blockIdx": 2
        },
        {
            "jpName": "ワンダーアキュート",
            "enName": "",
            "jpText": "そこそこは力もついたと思うのよ～。\nあたしも半人前くらいにはなれたかねぇ。",
            "enText": "",
            "nextBlock": 4,
            "origClipLength": 93,
            "choices": [
                {
                    "jpText": "ああ、目覚ましい伸びだ！",
                    "enText": "",
                    "nextBlock": 4
                },
                {
                    "jpText": "うん、目覚ましい伸びだよ！",
                    "enText": "",
                    "nextBlock": 4
