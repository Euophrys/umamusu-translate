            "nextBlock": 5,
            "origClipLength": 91,
            "animData": [
                {
                    "origLen": 64,
                    "pathId": 5692436322369581012
                },
                {
                    "origLen": 102,
                    "pathId": 8091424318755419011
                }
            ],
            "pathId": 340907206052516216,
            "blockIdx": 4
        },
        {
            "jpName": "",
            "enName": "",
            "jpText": "この調子で強くなっていけば、夏には――",
            "enText": "",
            "nextBlock": 6,
            "origClipLength": 69,
            "animData": [
                {
                    "origLen": 80,
                    "pathId": -7649084024542350666
