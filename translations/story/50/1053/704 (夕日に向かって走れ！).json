            "pathId": 1075545087556425654,
            "blockIdx": 19
        },
        {
            "jpName": "バンブーメモリー",
            "enName": "",
            "jpText": "ドラマ……はっ！？\nそうっス！",
            "enText": "",
            "nextBlock": 21,
            "origClipLength": 64,
            "animData": [
                {
                    "origLen": 48,
                    "pathId": 3967899195908759972
