            "pathId": -7289518842481178520,
            "blockIdx": 19
        },
        {
            "jpName": "地味なウマ娘",
            "enName": "",
            "jpText": "……っ。",
            "enText": "",
            "nextBlock": 21,
            "origClipLength": 50,
            "animData": [
