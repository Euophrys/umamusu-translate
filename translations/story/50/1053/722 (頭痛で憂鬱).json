        {
            "jpName": "バンブーメモリー",
            "enName": "",
            "jpText": "くっ……！",
            "enText": "",
            "nextBlock": 3,
            "origClipLength": 51,
            "choices": [
                {
                    "jpText": "大丈夫……？",
                    "enText": "",
                    "nextBlock": 3
