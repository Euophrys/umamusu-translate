            "origClipLength": 67,
            "animData": [
                {
                    "origLen": 25,
                    "pathId": -6512562213937962800
                },
                {
                    "origLen": 78,
                    "pathId": -4117735153660340162
                }
            ],
            "pathId": -273160036365101265,
            "blockIdx": 30
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "何が無様なの？　何が諦めろ、なの？",
            "enText": "What's so pathetic about it? What \ndo you want me to give up?",
