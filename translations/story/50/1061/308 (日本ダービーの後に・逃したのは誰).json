            "pathId": -585316437831652637,
            "blockIdx": 32
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "トレーナーッ！　聞きなさい！！",
            "enText": "Trainer! Listen to me!",
            "nextBlock": 34,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 17,
                    "pathId": -6503536026508064259
                },
                {
                    "origLen": 65,
                    "pathId": -2943717718481607242
                }
