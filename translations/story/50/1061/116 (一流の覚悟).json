            "blockIdx": 1
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "トレーナー。私、決めたわ。",
            "enText": "Trainer. I've made up my mind.",
            "nextBlock": 3,
            "origClipLength": 53,
            "animData": [
