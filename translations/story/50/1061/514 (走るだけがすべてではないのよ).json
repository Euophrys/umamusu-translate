        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "ちょっ！？　あなた余計なことを……っ！",
            "enText": "What? You're not supposed to…!",
            "nextBlock": 26,
            "origClipLength": 57,
            "choices": [
                {
                    "jpText": "…………",
                    "enText": "…",
                    "nextBlock": 26
                }
