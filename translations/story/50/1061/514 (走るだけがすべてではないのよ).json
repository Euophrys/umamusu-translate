            "nextBlock": 22,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 106,
                    "pathId": 8760799626420456967
                },
                {
                    "origLen": 138,
                    "pathId": 3700713976047699615
                },
                {
                    "origLen": 65,
                    "pathId": 115214121445702881
                },
                {
                    "origLen": 41,
