                }
            ],
            "pathId": 635913844888375057,
            "blockIdx": 19
        },
        {
            "jpName": "スペシャルウィーク",
            "enName": "Special Week",
            "jpText": "あれ、キングちゃん？",
            "enText": "Hey, King?",
            "nextBlock": 21,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 62,
                    "pathId": 2068921675898201088
                },
                {
