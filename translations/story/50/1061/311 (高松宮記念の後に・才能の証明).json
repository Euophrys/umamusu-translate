            "pathId": -7880893978734032409,
            "blockIdx": 6
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "私には……才能があった……！\r\nそうでしょう、トレーナー……っ！",
            "enText": "I had a… talent…! I \nknow you did, trainer…!",
            "nextBlock": 8,
            "origClipLength": 65,
            "choices": [
                {
                    "jpText": "ああ……！",
                    "enText": "Oh…!",
                    "nextBlock": 8
                },
                {
                    "jpText": "うん……！",
                    "enText": "Yeah!…!",
                    "nextBlock": 8
                }
            ],
            "animData": [
                {
                    "origLen": 61,
                    "pathId": -522423248893049721
                },
                {
                    "origLen": 76,
                    "pathId": 5494555143151997865
