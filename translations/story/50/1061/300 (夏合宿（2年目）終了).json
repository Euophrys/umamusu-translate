            "blockIdx": 3
        },
        {
            "jpName": "キングヘイロー",
            "enName": "King Halo",
            "jpText": "…………。",
            "enText": "…",
            "nextBlock": 5,
            "origClipLength": 48,
            "choices": [
                {
                    "jpText": "キング！！",
