            "enName": "",
            "jpText": "っつーワケでよ。",
            "enText": "",
            "nextBlock": 10,
            "origClipLength": 55,
            "choices": [
                {
                    "jpText": "下見しておきたいってことか",
                    "enText": "",
                    "nextBlock": 10
                },
                {
                    "jpText": "下見しておきたいってことだね",
                    "enText": "",
                    "nextBlock": 10
