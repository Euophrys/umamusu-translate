            "blockIdx": 38
        },
        {
            "jpName": "ナカヤマフェスタ",
            "enName": "",
            "jpText": "……だから、なんだよ。",
            "enText": "",
            "nextBlock": 40,
            "origClipLength": 59,
            "animData": [
                {
                    "origLen": 45,
                    "pathId": 8988180356536888528
