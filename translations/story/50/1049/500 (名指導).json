            "pathId": 614413782831984622,
            "blockIdx": 3
        },
        {
            "jpName": "ナカヤマフェスタ",
            "enName": "",
            "jpText": "ハァァァァァーーーッッ！",
            "enText": "",
            "nextBlock": 5,
            "origClipLength": 60,
            "animData": [
                {
                    "origLen": 22,
