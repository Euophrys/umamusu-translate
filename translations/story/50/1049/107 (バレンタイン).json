    "title": "バレンタイン",
    "text": [
        {
            "jpName": "実況",
            "enName": "",
            "jpText": "『ナカヤマフェスタ食らいつく！\n世界に繋がる扉はもう目の前だ！』",
            "enText": "",
            "nextBlock": 2,
            "origClipLength": 86,
            "pathId": 4941404149702960182,
            "blockIdx": 1
        },
        {
            "jpName": "実況",
            "enName": "",
            "jpText": "『がんばれ、ナカヤマフェスタ！！\nもうすぐだ！\nそのタイトルは手に届く場所にある！』",
            "enText": "",
            "nextBlock": 3,
            "origClipLength": 99,
            "pathId": 6387804884814125576,
            "blockIdx": 2
        },
        {
            "jpName": "実況",
            "enName": "",
            "jpText": "『ナカヤマフェスタ、2着ーっ！\nこれほどまでに凱旋門賞とは厳しいのか！』",
            "enText": "",
            "nextBlock": 4,
            "origClipLength": 91,
            "animData": [
                {
                    "origLen": 133,
                    "pathId": -1522690446694209316
                },
                {
                    "origLen": 133,
                    "pathId": 1283981254926973162
                },
                {
                    "origLen": 133,
                    "pathId": 1453535981335945594
                }
            ],
            "pathId": -4195093959587973502,
            "blockIdx": 3
        },
        {
            "jpName": "ナカヤマフェスタ",
            "enName": "",
            "jpText": "ハァッ……ハァッ……――\n畜生……ッ！",
            "enText": "",
            "nextBlock": 5,
            "origClipLength": 75,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 846744524741802148
                },
                {
                    "origLen": 52,
                    "pathId": -2010010586156406845
