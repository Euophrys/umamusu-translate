                },
                {
                    "origLen": 63,
                    "pathId": 8842497866573896638
                }
            ],
            "pathId": -5731521785945484659,
            "blockIdx": 34
        },
        {
            "jpName": "ナカヤマフェスタ",
            "enName": "",
            "jpText": "休め、とは言わないんだな。",
            "enText": "",
            "nextBlock": 36,
            "origClipLength": 61,
            "choices": [
                {
                    "jpText": "そう言っても、今は聞かないだろう？",
                    "enText": "",
                    "nextBlock": 36
                },
                {
                    "jpText": "そう言っても、今は聞かないでしょう？",
                    "enText": "",
                    "nextBlock": 36
                }
            ],
            "animData": [
                {
                    "origLen": 72,
                    "pathId": -5592422189050766611
                },
                {
                    "origLen": 72,
                    "pathId": 5061572464432474565
                }
