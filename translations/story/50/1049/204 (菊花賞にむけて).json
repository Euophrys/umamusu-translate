            "blockIdx": 4
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（コンコン）",
            "enText": "",
            "nextBlock": 6,
            "origClipLength": 52,
            "choices": [
                {
                    "jpText": "はい、どうぞ",
