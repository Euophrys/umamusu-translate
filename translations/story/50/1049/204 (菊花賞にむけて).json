            "pathId": -7507845713340339388,
            "blockIdx": 22
        },
        {
            "jpName": "ナカヤマフェスタ",
            "enName": "",
            "jpText": "……そうだな。",
            "enText": "",
            "nextBlock": 24,
            "origClipLength": 54,
