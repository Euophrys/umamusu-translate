        },
        {
            "jpName": "シリウスシンボリ",
            "enName": "",
            "jpText": "どうも。",
            "enText": "",
            "nextBlock": 7,
            "origClipLength": 86,
            "choices": [
                {
                    "jpText": "えっ、シリウスシンボリ！？",
                    "enText": "",
                    "nextBlock": 7
                }
