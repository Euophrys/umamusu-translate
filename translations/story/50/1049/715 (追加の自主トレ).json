                {
                    "origLen": 93,
                    "pathId": -6349968875000448779
                }
            ],
            "pathId": 5914570302449773819,
            "blockIdx": 8
        },
        {
            "jpName": "ナカヤマフェスタ",
            "enName": "",
            "jpText": "……表だ。",
            "enText": "",
            "nextBlock": 10,
            "origClipLength": 51,
            "choices": [
                {
                    "jpText": "よし、もう1本頑張ろう！",
                    "enText": "",
                    "nextBlock": 10
