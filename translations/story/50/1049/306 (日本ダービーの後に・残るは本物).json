            "pathId": 1816880508538530371,
            "blockIdx": 27
        },
        {
            "jpName": "ナカヤマフェスタ",
            "enName": "",
            "jpText": "……！",
            "enText": "",
            "nextBlock": 29,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 17,
                    "pathId": -1744628904787377871
