    "title": "ジャパンCの後に・未来への灯",
    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "（うおおおおおお！）",
            "enText": "",
            "nextBlock": 2,
            "origClipLength": 58,
            "animData": [
                {
                    "origLen": 69,
                    "pathId": 2182404237051887408
