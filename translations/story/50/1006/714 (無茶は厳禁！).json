            "pathId": -7097319358719892115,
            "blockIdx": 9
        },
        {
            "jpName": "オグリキャップ",
            "enName": "Oguri Cap",
            "jpText": "すぐでも走り出して……。",
            "enText": "You can start running right away…",
            "nextBlock": 11,
            "origClipLength": 52,
            "trainerGender": "f",
            "animData": [
