            "pathId": 931132155594405234,
            "blockIdx": 23
        },
        {
            "jpName": "オグリキャップ",
            "enName": "Oguri Cap",
            "jpText": "ライバル……。",
            "enText": "Rivals…",
            "nextBlock": 25,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 19,
                    "pathId": 7123084462849917485
                },
                {
                    "origLen": 65,
                    "pathId": 590272846851925569
