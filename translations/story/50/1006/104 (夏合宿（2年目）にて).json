            "pathId": -1319559642901938596,
            "blockIdx": 6
        },
        {
            "jpName": "タマモクロス",
            "enName": "Tamamo Cross",
            "jpText": "ちょっと見てきたほうがええんちゃう？\r\n砂浜におったで！",
            "enText": "Shouldn't we go check it out? \nI saw you on the beach!",
            "nextBlock": 8,
            "origClipLength": 81,
            "choices": [
                {
                    "jpText": "ありがとう！",
                    "enText": "Thank you!",
                    "nextBlock": 8
                }
            ],
            "animData": [
                {
                    "origLen": 16,
                    "pathId": -3713812620978355049
                },
                {
                    "origLen": 75,
                    "pathId": 4905640243801432851
