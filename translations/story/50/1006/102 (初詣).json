            "pathId": 5617444715594933012,
            "blockIdx": 16
        },
        {
            "jpName": "オグリキャップ",
            "enName": "Oguri Cap",
            "jpText": "……あ。",
            "enText": "… Oh.",
            "nextBlock": 18,
            "origClipLength": 47,
            "choices": [
                {
                    "jpText": "どうした？",
                    "enText": "What's wrong?",
                    "nextBlock": 18
                },
                {
                    "jpText": "どうしたの？",
                    "enText": "What's going on?",
                    "nextBlock": 18
                }
