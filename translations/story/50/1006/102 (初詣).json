    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "いよいよシニア級に挑戦するオグリキャップ。\r\n今年も無事に走れることを祈り、\n初詣に来たのだが――",
            "enText": "It's finally time to challenge the senior \nclass. I came to pay my first visit to the \nshrine, praying for a year without issues.",
            "nextBlock": 2,
            "origClipLength": 76,
            "coloredText": [
                {
                    "jpText": "シニア級",
                    "enText": "Senior class"
                }
            ],
            "animData": [
                {
                    "origLen": 115,
                    "pathId": -4434917103105918248
                },
                {
                    "origLen": 87,
                    "pathId": 6194624227070331188
                }
            ],
            "pathId": 8433071473248229017,
            "blockIdx": 1
        },
        {
            "jpName": "オグリキャップ",
            "enName": "Oguri Cap",
            "jpText": "……。",
            "enText": "…",
            "nextBlock": 3,
            "origClipLength": 46,
            "choices": [
                {
                    "jpText": "考え事か？",
                    "enText": "You thinking?",
                    "nextBlock": 3
                },
                {
                    "jpText": "考え事？",
                    "enText": "Thinking?",
                    "nextBlock": 3
                }
            ],
            "animData": [
