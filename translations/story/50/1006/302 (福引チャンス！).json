            "pathId": 904623425057578771,
            "blockIdx": 19
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "『<itemname>』を獲得した",
            "enText": "I got \"<itemname>\".",
            "nextBlock": 21,
