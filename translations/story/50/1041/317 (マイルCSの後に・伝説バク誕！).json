            "pathId": -6353339678248771145,
            "blockIdx": 6
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "ああ……素晴らしい！",
            "enText": "Oh… great!",
            "nextBlock": 8,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 12,
                    "pathId": -150476925434823274
                },
                {
                    "origLen": 62,
                    "pathId": 7648573309087299802
