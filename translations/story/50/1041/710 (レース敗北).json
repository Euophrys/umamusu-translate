            "blockIdx": 3
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "……ガクッ。",
            "enText": "… gulp.",
            "nextBlock": 5,
            "origClipLength": 52,
            "choices": [
                {
                    "jpText": "失敗は悪いことじゃない",
                    "enText": "Failure is not a bad thing.",
                    "nextBlock": 5
                },
                {
                    "jpText": "学級委員長、しっかり！",
                    "enText": "Class president, pull yourself together!",
                    "nextBlock": 9
