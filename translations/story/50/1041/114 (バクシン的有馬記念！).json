            "blockIdx": 28
        },
        {
            "jpName": "サクラバクシンオー",
            "enName": "Sakura Bakushin O",
            "jpText": "学級委員長であることです！！",
            "enText": "It's being the class president!",
            "nextBlock": 30,
            "origClipLength": 54,
            "choices": [
                {
                    "jpText": "違う！",
                    "enText": "No!",
                    "nextBlock": 30
                }
