            "enName": "Sakura Bakushin O",
            "jpText": "ハイッ！",
            "enText": "Hi!",
            "nextBlock": 29,
            "origClipLength": 47,
            "choices": [
                {
                    "jpText": "『有馬記念』を走るのに、1番大事なのは？",
                    "enText": "What's the most important thing about\nrunning \"Arima Kinen\"?",
                    "nextBlock": 29
                }
            ],
            "animData": [
                {
                    "origLen": 34,
                    "pathId": -1629466464404253219
                },
                {
                    "origLen": 58,
