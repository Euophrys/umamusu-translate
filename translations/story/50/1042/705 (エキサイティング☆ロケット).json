            "blockIdx": 2
        },
        {
            "jpName": "シーキングザパール",
            "enName": "",
            "jpText": "……！！\nあれは……ロケット花火！？",
            "enText": "",
            "nextBlock": 4,
            "origClipLength": 68,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 4003834253358809035
                },
                {
                    "origLen": 58,
                    "pathId": -3726159578699321146
