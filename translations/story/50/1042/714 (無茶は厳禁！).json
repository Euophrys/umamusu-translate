            "pathId": -2867822255299604147,
            "blockIdx": 3
        },
        {
            "jpName": "シーキングザパール",
            "enName": "",
            "jpText": "だから――",
            "enText": "",
            "nextBlock": 5,
            "origClipLength": 51,
