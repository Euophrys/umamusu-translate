            "blockIdx": 5
        },
        {
            "jpName": "シーキングザパール",
            "enName": "",
            "jpText": "フッ！！　ハッ！！　セイヤッ！！",
            "enText": "",
            "nextBlock": 7,
            "origClipLength": 65,
            "animData": [
