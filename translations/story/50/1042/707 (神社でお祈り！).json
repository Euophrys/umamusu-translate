            "blockIdx": 2
        },
        {
            "jpName": "シーキングザパール",
            "enName": "",
            "jpText": "神さまを喜ばせに来たのよ！",
            "enText": "",
            "nextBlock": 4,
            "origClipLength": 61,
            "choices": [
                {
                    "jpText": "どういうこと……？",
                    "enText": "",
                    "nextBlock": 4
