            "pathId": -3066747661779106425,
            "blockIdx": 3
        },
        {
            "jpName": "シーキングザパール",
            "enName": "",
            "jpText": "けれど――ごめんなさい！\n今日はやめておくわ！",
            "enText": "",
            "nextBlock": 5,
            "origClipLength": 75,
            "choices": [
                {
                    "jpText": "えっ、いらないのか？",
                    "enText": "",
                    "nextBlock": 5
                },
                {
                    "jpText": "えっ、いらないの？",
                    "enText": "",
                    "nextBlock": 5
                }
