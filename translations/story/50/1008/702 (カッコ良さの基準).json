                },
                {
                    "origLen": 66,
                    "pathId": -1260733147066843946
                }
            ],
            "pathId": 7157290272340965553,
            "blockIdx": 17
        },
        {
            "jpName": "スタッフA",
            "enName": "Staff A",
            "jpText": "いつもの……ですか？",
            "enText": "Is it the usual…?",
            "nextBlock": 19,
            "origClipLength": 58,
            "animData": [
                {
                    "origLen": 34,
                    "pathId": -5218995768973806521
                },
                {
                    "origLen": 69,
