            "animData": [
                {
                    "origLen": 131,
                    "pathId": -6098363309135606153
                },
                {
                    "origLen": 131,
                    "pathId": 5247303482911861294
                }
            ],
            "pathId": -3820419071390791063,
            "blockIdx": 2
        },
        {
            "jpName": "ウオッカ",
            "enName": "",
            "jpText": "おお、よくわかったな！　カッケー言葉で\nバシッと伝わると、気持ちいいぜ～！",
            "enText": "",
            "nextBlock": 4,
            "origClipLength": 93,
            "animData": [
