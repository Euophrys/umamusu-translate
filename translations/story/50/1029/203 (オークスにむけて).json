            "jpName": "ユキノビジン",
            "enName": "",
            "jpText": "……。",
            "enText": "",
            "nextBlock": 47,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 25,
                    "pathId": -263902635835102021
                },
