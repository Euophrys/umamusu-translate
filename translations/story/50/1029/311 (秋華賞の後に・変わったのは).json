            "pathId": 2794616399413098339,
            "blockIdx": 4
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "",
            "jpText": "……ユキノ。",
            "enText": "",
            "nextBlock": 6,
            "origClipLength": 52,
            "animData": [
