                {
                    "origLen": 109,
                    "pathId": -8338236664166677115
                }
            ],
            "pathId": 555469729355059475,
            "blockIdx": 27
        },
        {
            "jpName": "ユキノビジン",
            "enName": "",
            "jpText": "ぜ、絶対、ですか……？",
            "enText": "",
            "nextBlock": 29,
            "origClipLength": 66,
            "choices": [
                {
                    "jpText": "なんでも頑張って取り組むユキノなら！",
                    "enText": "",
                    "nextBlock": 29
