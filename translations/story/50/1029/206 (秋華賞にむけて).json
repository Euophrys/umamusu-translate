            "blockIdx": 14
        },
        {
            "jpName": "ユキノビジン",
            "enName": "",
            "jpText": "じゃああたし、\n誰に向けて走ればいいンだ……？",
            "enText": "",
            "nextBlock": 16,
            "origClipLength": 75,
            "animData": [
                {
                    "origLen": 28,
                    "pathId": -1130450864499672668
