            "pathId": 8707575039731635690,
            "blockIdx": 13
        },
        {
            "jpName": "ユキノビジン",
            "enName": "",
            "jpText": "……そンな。",
            "enText": "",
            "nextBlock": 15,
            "origClipLength": 52,
