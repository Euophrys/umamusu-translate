            "origClipLength": 85,
            "animData": [
                {
                    "origLen": 63,
                    "pathId": -1372778351445502008
                },
                {
                    "origLen": 96,
                    "pathId": 3342515110058974678
                }
            ],
            "pathId": 3281175361766324950,
            "blockIdx": 26
        },
        {
            "jpName": "ユキノビジン",
            "enName": "",
            "jpText": "『秋華賞』でも、\nがんばって勝てばいいンだな！",
