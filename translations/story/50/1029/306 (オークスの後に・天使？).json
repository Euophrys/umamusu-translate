            "blockIdx": 18
        },
        {
            "jpName": "ユキノビジン",
            "enName": "",
            "jpText": "あたしとあの子、\n何が違うンだろ――",
            "enText": "",
            "nextBlock": 20,
            "origClipLength": 68,
            "animData": [
                {
                    "origLen": 25,
                    "pathId": 9171422228819331355
