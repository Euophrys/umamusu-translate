            "blockIdx": 6
        },
        {
            "jpName": "ユキノビジン",
            "enName": "",
            "jpText": "ンだなはん、\nみなさんあったかかったぁ。",
            "enText": "",
            "nextBlock": 8,
            "origClipLength": 71,
            "animData": [
                {
                    "origLen": 29,
