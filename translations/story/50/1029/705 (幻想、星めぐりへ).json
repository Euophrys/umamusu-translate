            "animData": [
                {
                    "origLen": 59,
                    "pathId": -1904558720091872927
                },
                {
                    "origLen": 102,
                    "pathId": 160295014952002638
                }
            ],
            "pathId": 6020202536562744580,
            "blockIdx": 33
        },
        {
            "jpName": "ユキノビジン",
            "enName": "",
            "jpText": "ふふふ……なら、\nもうちょっと預かっててもいいですかね？",
            "enText": "",
            "nextBlock": 35,
            "origClipLength": 81,
            "animData": [
                {
                    "origLen": 64,
                    "pathId": -5464117793353476994
                },
                {
                    "origLen": 92,
                    "pathId": -7797278440462662400
                }
            ],
            "pathId": -2729228955950374490,
            "blockIdx": 34
        },
        {
            "jpName": "ユキノビジン",
            "enName": "",
            "jpText": "まだまだたくさん、お話したいですし……！",
            "enText": "",
            "nextBlock": -1,
            "origClipLength": 71,
            "animData": [
                {
                    "origLen": 82,
                    "pathId": -1571797901640361690
                },
                {
                    "origLen": 82,
                    "pathId": 4058434455767895108
                }
