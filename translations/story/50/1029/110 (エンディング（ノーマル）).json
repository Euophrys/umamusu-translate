                {
                    "origLen": 61,
                    "pathId": -6970525232758505020
                },
                {
                    "origLen": 61,
                    "pathId": -1153520694778278905
                }
            ],
            "pathId": -3794969828015075886,
            "blockIdx": 20
        },
        {
            "jpName": "ユキノビジン",
            "enName": "",
            "jpText": "あ！　ンだなはん！\nいただきます！",
            "enText": "",
            "nextBlock": 22,
            "origClipLength": 67,
            "animData": [
                {
                    "origLen": 110,
                    "pathId": 1315879789059751502
                },
                {
                    "origLen": 117,
                    "pathId": 7204972629056585772
                },
                {
                    "origLen": 118,
                    "pathId": 1733838551590534636
                }
            ],
            "pathId": 7849900446959435472,
            "blockIdx": 21
        },
        {
            "jpName": "ユキノビジン",
            "enName": "",
            "jpText": "――オシャレも、\n心なンですねぇ。",
            "enText": "",
            "nextBlock": 23,
            "origClipLength": 67,
            "choices": [
                {
                    "jpText": "え？",
                    "enText": "",
                    "nextBlock": 23
