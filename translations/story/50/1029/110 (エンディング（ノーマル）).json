            "pathId": 2884819447618537326,
            "blockIdx": 2
        },
        {
            "jpName": "ユキノビジン",
            "enName": "",
            "jpText": "はいっ。",
            "enText": "",
            "nextBlock": 4,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 102,
                    "pathId": 1281064609106142185
