                {
                    "origLen": 82,
                    "pathId": -2021878440413783378
                }
            ],
            "pathId": 2778067891382691699,
            "blockIdx": 28
        },
        {
            "jpName": "ヤマニンゼファー",
            "enName": "",
            "jpText": "なかなか、長い風待ち……ですね。",
            "enText": "",
            "nextBlock": 30,
            "origClipLength": 65,
            "choices": [
                {
                    "jpText": "……もどかしい？",
                    "enText": "",
                    "nextBlock": 30
                }
            ],
            "animData": [
                {
                    "origLen": 38,
                    "pathId": 2192024871584994436
                },
