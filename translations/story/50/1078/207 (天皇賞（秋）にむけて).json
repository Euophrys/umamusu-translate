            "nextBlock": 6,
            "origClipLength": 93,
            "pathId": -626112159319976552,
            "blockIdx": 5
        },
        {
            "jpName": "幼いゼファー",
            "enName": "",
            "jpText": "……はい、わかりました。",
            "enText": "",
            "nextBlock": 7,
            "origClipLength": 60,
            "pathId": 4611118581110356785,
            "blockIdx": 6
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（ビュゥッ――……！）",
