            "animData": [
                {
                    "origLen": 114,
                    "pathId": -4489529092028327220
                },
                {
                    "origLen": 114,
                    "pathId": 4512687733581876217
                }
            ],
            "pathId": 7178246863001549433,
            "blockIdx": 20
        },
        {
            "jpName": "ヤマニンゼファー",
            "enName": "",
            "jpText": "落ち着くまで……。",
            "enText": "",
            "nextBlock": 22,
            "origClipLength": 56,
