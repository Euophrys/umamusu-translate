            "blockIdx": 25
        },
        {
            "jpName": "ヤマニンゼファー",
            "enName": "",
            "jpText": "……行きたくありませんか？",
            "enText": "",
            "nextBlock": 27,
            "origClipLength": 61,
            "choices": [
