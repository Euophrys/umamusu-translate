            "pathId": -5074082760022673996,
            "blockIdx": 19
        },
        {
            "jpName": "マンハッタンカフェ",
            "enName": "Manhattan Cafe",
            "jpText": "はぁ、はぁ、はぁ……。",
            "enText": "Ha, ha, ha…",
            "nextBlock": 21,
            "origClipLength": 159,
            "animData": [
                {
                    "origLen": 23,
                    "pathId": 845472292527988648
                },
                {
                    "origLen": 329,
                    "pathId": 7411400474992582058
                },
                {
                    "origLen": 274,
                    "pathId": -5367127979814659399
