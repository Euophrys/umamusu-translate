            "blockIdx": 24
        },
        {
            "jpName": "マンハッタンカフェ",
            "enName": "Manhattan Cafe",
            "jpText": "……………………。",
            "enText": "…",
            "nextBlock": 26,
            "origClipLength": 56,
            "animData": [
                {
                    "origLen": 20,
