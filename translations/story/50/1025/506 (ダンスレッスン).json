            "blockIdx": 6
        },
        {
            "jpName": "マンハッタンカフェ",
            "enName": "Manhattan Cafe",
            "jpText": "…………。",
            "enText": "…",
            "nextBlock": 8,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 24,
                    "pathId": 2292655184532550805
