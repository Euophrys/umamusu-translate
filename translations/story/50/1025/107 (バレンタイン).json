            "pathId": -1575785411911934548,
            "blockIdx": 2
        },
        {
            "jpName": "マンハッタンカフェ",
            "enName": "Manhattan Cafe",
            "jpText": "……見えますか？",
            "enText": "Can you see…?",
            "nextBlock": 4,
            "origClipLength": 55,
            "choices": [
                {
                    "jpText": "見えません……",
                    "enText": "Not visible…",
                    "nextBlock": 4
                }
