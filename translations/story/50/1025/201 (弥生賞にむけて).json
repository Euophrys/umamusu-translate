            "blockIdx": 9
        },
        {
            "jpName": "マンハッタンカフェ",
            "enName": "Manhattan Cafe",
            "jpText": "つまり……？",
            "enText": "I mean…?",
            "nextBlock": 11,
            "origClipLength": 52,
