            "pathId": -4301902326032074039,
            "blockIdx": 1
        },
        {
            "jpName": "マンハッタンカフェ",
            "enName": "Manhattan Cafe",
            "jpText": "……トレーナーさん。",
            "enText": "… Trainer.",
            "nextBlock": 3,
            "origClipLength": 58,
            "animData": [
                {
                    "origLen": 26,
                    "pathId": 7797833037664790651
                },
