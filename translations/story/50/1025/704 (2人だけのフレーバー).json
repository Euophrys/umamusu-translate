            "blockIdx": 30
        },
        {
            "jpName": "マンハッタンカフェ",
            "enName": "Manhattan Cafe",
            "jpText": "でも――――",
            "enText": "But —",
            "nextBlock": 32,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 26,
                    "pathId": 8264692898042453577
                },
                {
