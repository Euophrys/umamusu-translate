            "pathId": -6598134346520676759,
            "blockIdx": 29
        },
        {
            "jpName": "ライスシャワー",
            "enName": "Rice Shower",
            "jpText": "だ……だめだよぉ……！",
            "enText": "No… no…!",
            "nextBlock": 31,
            "origClipLength": 76,
            "animData": [
                {
                    "origLen": 43,
                    "pathId": 2642125691926487080
                },
