            "pathId": -215380377614407848,
            "blockIdx": 14
        },
        {
            "jpName": "ウマ娘たち",
            "enName": "Horsegirls",
            "jpText": "キャ～～～～ッ！！　クール～～ッ！",
            "enText": "Ca ～～～～! Cool~!",
            "nextBlock": 16,
            "origClipLength": 56,
            "animData": [
                {
                    "origLen": 59,
                    "pathId": -2161374033644053043
                },
                {
                    "origLen": 59,
