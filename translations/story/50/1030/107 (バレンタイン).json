            "blockIdx": 18
        },
        {
            "jpName": "ライスシャワー",
            "enName": "Rice Shower",
            "jpText": "本当に……大丈夫……？",
            "enText": "Ben Dang Ni… 大丈夫…?",
            "nextBlock": 20,
            "origClipLength": 75,
            "trainerGender": "f",
            "animData": [
                {
                    "origLen": 46,
                    "pathId": -7281168430619365557
