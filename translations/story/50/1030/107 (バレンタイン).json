            "blockIdx": 20
        },
        {
            "jpName": "ライスシャワー",
            "enName": "Rice Shower",
            "jpText": "……うんっ！",
            "enText": "… Yeah!",
            "nextBlock": 22,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 26,
                    "pathId": -1034705972030398297
                },
                {
                    "origLen": 59,
