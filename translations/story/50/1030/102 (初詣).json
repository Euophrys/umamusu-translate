            "blockIdx": 7
        },
        {
            "jpName": "ライスシャワー",
            "enName": "Rice Shower",
            "jpText": "……強く、なりたいから。",
            "enText": "… because I want to be strong.",
            "nextBlock": 9,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 63,
