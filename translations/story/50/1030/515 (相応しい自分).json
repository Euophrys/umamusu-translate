            "blockIdx": 6
        },
        {
            "jpName": "ライスシャワー",
            "enName": "Rice Shower",
            "jpText": "は、はいっ！？",
            "enText": "Ha, ha!",
            "nextBlock": 8,
            "origClipLength": 49,
            "animData": [
                {
                    "origLen": 28,
