            "jpName": "ライスシャワー",
            "enName": "Rice Shower",
            "jpText": "お兄さま――",
            "enText": "Brother—",
            "nextBlock": 33,
            "origClipLength": 48,
            "trainerGender": "m",
            "animData": [
                {
                    "origLen": 22,
                    "pathId": -8595793389774657508
