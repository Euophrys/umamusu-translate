            "pathId": 6408572308196525102,
            "blockIdx": 2
        },
        {
            "jpName": "ライスシャワー",
            "enName": "Rice Shower",
            "jpText": "あっ――\r\n見て、お姉さま！",
            "enText": "Oh— look, sister!",
            "nextBlock": 4,
            "origClipLength": 54,
            "trainerGender": "f",
            "animData": [
