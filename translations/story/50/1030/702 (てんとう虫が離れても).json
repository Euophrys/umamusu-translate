            "pathId": 78612541756663242,
            "blockIdx": 1
        },
        {
            "jpName": "ライスシャワー",
            "enName": "Rice Shower",
            "jpText": "あっ――\r\n見て、お兄さま！",
            "enText": "Oh— look, big brother!",
            "nextBlock": 3,
            "origClipLength": 54,
            "trainerGender": "m",
