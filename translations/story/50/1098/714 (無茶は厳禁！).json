            "pathId": -5820761487698123327,
            "blockIdx": 18
        },
        {
            "jpName": "コパノリッキー",
            "enName": "",
            "jpText": "ほっ、ほっ、ほっ……！",
            "enText": "",
            "nextBlock": 20,
            "origClipLength": 59,
            "trainerGender": "f",
            "animData": [
