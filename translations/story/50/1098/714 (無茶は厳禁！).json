                {
                    "origLen": 114,
                    "pathId": -3891547813120865989
                }
            ],
            "pathId": -1103981351136277260,
            "blockIdx": 13
        },
        {
            "jpName": "コパノリッキー",
            "enName": "",
            "jpText": "……ふっ、ふっ、……くうっ！",
            "enText": "",
            "nextBlock": 15,
            "origClipLength": 63,
            "trainerGender": "f",
            "animData": [
                {
                    "origLen": 43,
                    "pathId": 286234411119075899
                },
                {
                    "origLen": 74,
                    "pathId": -958615857025320929
                }
