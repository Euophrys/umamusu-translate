            "pathId": 2662686624960652321,
            "blockIdx": 20
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "（キィィィン！）",
            "enText": "",
            "nextBlock": 22,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 19,
                    "pathId": -8292775738318462252
