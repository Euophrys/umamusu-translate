            "animData": [
                {
                    "origLen": 80,
                    "pathId": 1287447926247205471
                },
                {
                    "origLen": 212,
                    "pathId": -3589799865863532954
                }
            ],
            "pathId": 4078558484840770072,
            "blockIdx": 21
        },
        {
            "jpName": "コパノリッキー",
            "enName": "",
            "jpText": "あ、そうだね。直接聞けばわかるかな？",
            "enText": "",
            "nextBlock": 23,
            "origClipLength": 68,
            "animData": [
