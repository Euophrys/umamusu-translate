            "pathId": 6385287316638034896,
            "blockIdx": 20
        },
        {
            "jpName": "ウマ娘B",
            "enName": "",
            "jpText": "私たちはそういうのはいいから。\nまたね。",
            "enText": "",
            "nextBlock": 22,
            "origClipLength": 71,
            "animData": [
                {
                    "origLen": 89,
                    "pathId": 8755312060991704805
                },
