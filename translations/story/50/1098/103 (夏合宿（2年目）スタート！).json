            "pathId": -3012209086915405868,
            "blockIdx": 21
        },
        {
            "jpName": "コパノリッキー",
            "enName": "",
            "jpText": "あ……！",
            "enText": "",
            "nextBlock": 23,
            "origClipLength": 50,
            "animData": [
                {
                    "origLen": 37,
                    "pathId": -5309256014189629455
                },
