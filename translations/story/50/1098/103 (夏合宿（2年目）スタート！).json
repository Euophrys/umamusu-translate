            "pathId": -6947080090096363269,
            "blockIdx": 4
        },
        {
            "jpName": "コパノリッキー",
            "enName": "",
            "jpText": "あ、もしかして――これのこと？",
            "enText": "",
            "nextBlock": 6,
            "origClipLength": 64,
            "choices": [
                {
                    "jpText": "……どれのこと？",
                    "enText": "",
                    "nextBlock": 6
                }
