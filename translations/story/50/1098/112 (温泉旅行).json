            "blockIdx": 2
        },
        {
            "jpName": "コパノリッキー",
            "enName": "",
            "jpText": "リッキー☆　ラッキー☆　温泉ツアー！！",
            "enText": "",
            "nextBlock": 4,
            "origClipLength": 69,
            "choices": [
                {
