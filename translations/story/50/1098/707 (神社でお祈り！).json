            "pathId": -3867369891643944007,
            "blockIdx": 18
        },
        {
            "jpName": "コパノリッキー",
            "enName": "",
            "jpText": "――ん？",
            "enText": "",
            "nextBlock": 20,
            "origClipLength": 50,
            "trainerGender": "f",
            "animData": [
                {
                    "origLen": 27,
                    "pathId": -670798732666874049
