                {
                    "origLen": 159,
                    "pathId": 8277600503504599992
                }
            ],
            "pathId": 4542257893528047031,
            "blockIdx": 21
        },
        {
            "jpName": "コパノリッキー",
            "enName": "",
            "jpText": "でも……こんな時こそありがとう、だよね。",
            "enText": "",
            "nextBlock": 23,
            "origClipLength": 71,
            "animData": [
                {
                    "origLen": 33,
                    "pathId": 5035166852488068329
                },
