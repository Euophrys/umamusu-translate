            "animData": [
                {
                    "origLen": 67,
                    "pathId": -2391772522457860327
                },
                {
                    "origLen": 87,
                    "pathId": 5154399490808199191
                }
            ],
            "pathId": -451404630471689887,
            "blockIdx": 16
        },
        {
            "jpName": "コパノリッキー",
            "enName": "",
            "jpText": "ふふっ、お互いありがとうで、大吉祥だね☆",
            "enText": "",
            "nextBlock": 18,
            "origClipLength": 71,
            "animData": [
                {
                    "origLen": 40,
                    "pathId": -4443600026827592809
                },
                {
                    "origLen": 82,
                    "pathId": 4938499118887223099
                }
            ],
            "pathId": 4792184369334650608,
            "blockIdx": 17
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "その後も道行く物事に\n2人でありがとうと言いながら、\nハッピーな気分で帰路を歩いたのだった。",
            "enText": "",
            "nextBlock": -1,
            "origClipLength": 105,
            "animData": [
                {
                    "origLen": 14,
                    "pathId": 6322254679729619999
