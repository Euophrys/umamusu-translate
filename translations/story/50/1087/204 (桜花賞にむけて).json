            "pathId": -8599854941353963889,
            "blockIdx": 16
        },
        {
            "jpName": "アストンマーチャン",
            "enName": "",
            "jpText": "…………。",
            "enText": "",
            "nextBlock": 18,
            "origClipLength": 51,
            "animData": [
