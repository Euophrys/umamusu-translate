            "pathId": 623478676271370265,
            "blockIdx": 17
        },
        {
            "jpName": "アストンマーチャン",
            "enName": "",
            "jpText": "……むぅ……。",
            "enText": "",
            "nextBlock": 19,
            "origClipLength": 54,
            "choices": [
                {
                    "jpText": "……落ち着かない？",
                    "enText": "",
                    "nextBlock": 19
                }
            ],
            "animData": [
                {
                    "origLen": 76,
                    "pathId": -7907249788642724074
                },
                {
                    "origLen": 65,
                    "pathId": 4836704961253145825
                },
                {
                    "origLen": 65,
                    "pathId": 2290997809487844746
                }
            ],
            "pathId": 2761535247321066563,
            "blockIdx": 18
        },
        {
            "jpName": "アストンマーチャン",
            "enName": "",
            "jpText": "はい。",
            "enText": "",
            "nextBlock": 20,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 21,
                    "pathId": 937223870700613602
