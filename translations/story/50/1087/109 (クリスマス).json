            "animData": [
                {
                    "origLen": 89,
                    "pathId": 4098363424277059717
                },
                {
                    "origLen": 65,
                    "pathId": -2985279410355605867
                }
            ],
            "pathId": -982166011881679471,
            "blockIdx": 28
        },
        {
            "jpName": "アストンマーチャン",
            "enName": "",
            "jpText": "……よかった。\nなによりです。",
            "enText": "",
            "nextBlock": 30,
            "origClipLength": 64,
            "animData": [
