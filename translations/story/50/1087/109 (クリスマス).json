            "animData": [
                {
                    "origLen": 119,
                    "pathId": 5280920199131909010
                },
                {
                    "origLen": 138,
                    "pathId": 5634638309571446497
                }
            ],
            "pathId": 5857272480054691460,
            "blockIdx": 5
        },
        {
            "jpName": "アストンマーチャン",
            "enName": "",
            "jpText": "よい質問です。",
            "enText": "",
            "nextBlock": 7,
            "origClipLength": 54,
            "animData": [
                {
                    "origLen": 65,
                    "pathId": 7785036365868830078
