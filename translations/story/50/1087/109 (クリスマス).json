    "text": [
        {
            "jpName": "",
            "enName": "",
            "jpText": "クリスマスの朝。\nちいさなノック音で目が覚めた。",
            "enText": "",
            "nextBlock": 2,
            "origClipLength": 76,
            "pathId": -877273406254984283,
            "blockIdx": 1
        },
        {
            "jpName": "",
            "enName": "",
