            "pathId": -5781291036924867062,
            "blockIdx": 36
        },
        {
            "jpName": "アストンマーチャン",
            "enName": "",
            "jpText": "というわけで……\nまたね～。",
            "enText": "",
            "nextBlock": 38,
            "origClipLength": 132,
            "animData": [
                {
                    "origLen": 98,
                    "pathId": 536004978009649594
                },
                {
                    "origLen": 98,
                    "pathId": -6683484331404852122
                }
