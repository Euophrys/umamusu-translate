            "pathId": -2815982059863846474,
            "blockIdx": 22
        },
        {
            "jpName": "アストンマーチャン",
            "enName": "",
            "jpText": "あいさつに行ってきます。",
            "enText": "",
            "nextBlock": 24,
            "origClipLength": 80,
            "choices": [
                {
                    "jpText": "え？あ、ああ……",
                    "enText": "",
                    "nextBlock": 24
                },
                {
                    "jpText": "え？あ、うん……",
                    "enText": "",
                    "nextBlock": 24
                }
