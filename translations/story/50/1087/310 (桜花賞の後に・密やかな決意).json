            "pathId": 2670443114463529596,
            "blockIdx": 21
        },
        {
            "jpName": "アストンマーチャン",
            "enName": "",
            "jpText": "……安心しました。",
            "enText": "",
            "nextBlock": 23,
            "origClipLength": 56,
            "animData": [
                {
                    "origLen": 29,
                    "pathId": -4177321351375542680
