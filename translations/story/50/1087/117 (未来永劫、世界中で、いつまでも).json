                {
                    "origLen": 123,
                    "pathId": 7603313569464410329
                }
            ],
            "pathId": 8969947339783136991,
            "blockIdx": 34
        },
        {
            "jpName": "編集者",
            "enName": "",
            "jpText": "ええ、そういうことです！",
            "enText": "",
            "nextBlock": 36,
            "origClipLength": 60,
            "animData": [
