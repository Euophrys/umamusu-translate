            "animData": [
                {
                    "origLen": 58,
                    "pathId": -1813497554453940252
                },
                {
                    "origLen": 91,
                    "pathId": 3579993506399509051
                }
            ],
            "pathId": -5555471207569219084,
            "blockIdx": 7
        },
        {
            "jpName": "アストンマーチャン",
            "enName": "",
            "jpText": "見送りたい。……？",
            "enText": "",
            "nextBlock": 9,
            "origClipLength": 56,
            "animData": [
                {
                    "origLen": 25,
                    "pathId": 4074784690141298313
