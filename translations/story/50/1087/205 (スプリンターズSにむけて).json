            "nextBlock": 7,
            "origClipLength": 55,
            "animData": [
                {
                    "origLen": 21,
                    "pathId": 3465211675929701756
                },
                {
                    "origLen": 20,
                    "pathId": -8334138565061570703
                },
                {
                    "origLen": 66,
                    "pathId": -7660230026040798909
