            "pathId": 480840922296494279,
            "blockIdx": 10
        },
        {
            "jpName": "秋川理事長",
            "enName": "",
            "jpText": "では、これにてっ！",
            "enText": "",
            "nextBlock": 12,
            "origClipLength": 56,
            "animData": [
                {
                    "origLen": 59,
                    "pathId": -6788093968717290149
                },
                {
                    "origLen": 59,
                    "pathId": 7858590135366701690
