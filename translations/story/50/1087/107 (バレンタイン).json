            "pathId": 7982237927957578178,
            "blockIdx": 23
        },
        {
            "jpName": "モノローグ",
            "enName": "",
            "jpText": "と、いうわけで。",
            "enText": "",
            "nextBlock": 25,
            "origClipLength": 55,
