            "pathId": 6903664027542297111,
            "blockIdx": 4
        },
        {
            "jpName": "マルゼンスキー",
            "enName": "Maruzensky",
            "jpText": "あら、トレーナーちゃん！",
            "enText": "Hey, trainer! ",
            "nextBlock": 6,
            "origClipLength": 60,
            "trainerGender": "f",
