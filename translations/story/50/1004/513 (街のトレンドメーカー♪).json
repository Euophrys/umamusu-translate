            "animData": [
                {
                    "origLen": 74,
                    "pathId": 2041354399151560137
                },
                {
                    "origLen": 108,
                    "pathId": -1293932372993869282
                }
            ],
            "pathId": 4561444763354652008,
            "blockIdx": 30
        },
        {
            "jpName": "マルゼンスキー",
            "enName": "Maruzensky",
            "jpText": "流行を追うより、自分から発信する方が\nきっと楽しいし。わかったわ、やってみる！",
            "enText": "I'm sure it's more fun to send\nout messages than to follow trends.\nAll right, I'll try!",
            "nextBlock": 32,
            "origClipLength": 97,
            "animData": [
                {
                    "origLen": 78,
                    "pathId": 3356998216841200399
                },
                {
                    "origLen": 108,
                    "pathId": -2654950097980607581
                }
            ],
            "pathId": 5430519743888633974,
            "blockIdx": 31
        },
