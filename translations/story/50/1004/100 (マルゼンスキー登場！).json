            "animData": [
                {
                    "origLen": 106,
                    "pathId": -3597499238395476828
                },
                {
                    "origLen": 102,
                    "pathId": 9145904179820055823
                },
                {
                    "origLen": 102,
                    "pathId": 8525664573269161550
                }
