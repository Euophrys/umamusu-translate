            "pathId": -4648507244785666614,
            "blockIdx": 14
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "ふーーーーーん……。",
            "enText": "Woohoo…",
            "nextBlock": 16,
            "origClipLength": 58,
            "animData": [
                {
                    "origLen": 21,
