        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "……バカにしてんの？\r\nアンタも、……母さんも。",
            "enText": "Are you making fun of…? Both \nyou and your… mother.",
            "nextBlock": 17,
            "origClipLength": 76,
            "choices": [
                {
                    "jpText": "えっ",
                    "enText": "What's up?",
                    "nextBlock": 17
                }
            ],
            "animData": [
                {
                    "origLen": 19,
                    "pathId": 2742634449694092027
