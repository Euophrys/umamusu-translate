            "pathId": -9020404972146830030,
            "blockIdx": 3
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "うるさい。\r\n別に……ビビってなんかないし。",
            "enText": "Shut up. I'm not… scared.",
            "nextBlock": 5,
            "origClipLength": 73,
            "choices": [
                {
                    "jpText": "でも――",
                    "enText": "But―",
                    "nextBlock": 5
                }
            ],
            "animData": [
                {
                    "origLen": 21,
                    "pathId": 8502023690343236085
                },
                {
                    "origLen": 73,
