                {
                    "origLen": 86,
                    "pathId": -2228228416932982866
                }
            ],
            "pathId": 6796104674564097308,
            "blockIdx": 13
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "……っ。",
            "enText": "…",
            "nextBlock": 15,
            "origClipLength": 50,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": -8476292796427232548
                },
                {
                    "origLen": 50,
