            "blockIdx": 5
        },
        {
            "jpName": "ナリタタイシン",
            "enName": "Narita Taishin",
            "jpText": "あ……ハイ。",
            "enText": "Oh… hi.",
            "nextBlock": 7,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 20,
                    "pathId": 5415648816487401151
                },
                {
                    "origLen": 55,
