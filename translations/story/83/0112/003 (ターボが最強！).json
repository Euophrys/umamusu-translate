            "animData": [
                {
                    "origLen": 142,
                    "pathId": -355328069537931109
                },
                {
                    "origLen": 142,
                    "pathId": 8199562131692739018
                }
            ],
            "pathId": -7880363658120937028,
            "blockIdx": 5
        },
        {
            "jpName": "ツインターボ",
            "enName": "",
            "jpText": "そう、そう、そうなの！\nハラがたつ！　ムカつくし……許せない！！",
            "enText": "",
            "nextBlock": 7,
            "origClipLength": 86,
            "animData": [
                {
                    "origLen": 72,
                    "pathId": -7960495915598950942
