            "pathId": -4490166695544070668,
            "blockIdx": 28
        },
        {
            "jpName": "オグリキャップ",
            "enName": "Oguri Cap",
            "jpText": "タマ……。",
            "enText": "Tama…",
            "nextBlock": 30,
            "origClipLength": 48,
            "animData": [
                {
                    "origLen": 64,
                    "pathId": 4549281578831068944
                },
                {
