            "pathId": -8825304695093623889,
            "blockIdx": 5
        },
        {
            "jpName": "ダイイチルビー",
            "enName": "",
            "jpText": "…………。",
            "enText": "",
            "nextBlock": 7,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 149,
                    "pathId": 6549194918820205004
