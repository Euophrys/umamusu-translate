            "pathId": -6622681606686759379,
            "blockIdx": 15
        },
        {
            "jpName": "ビワハヤヒデ",
            "enName": "Biwa Hayahide",
            "jpText": "なに……？",
            "enText": "What…?",
            "nextBlock": 17,
            "origClipLength": 51,
            "animData": [
                {
                    "origLen": 62,
                    "pathId": -7844931604264409089
                },
                {
                    "origLen": 62,
