                {
                    "origLen": 74,
                    "pathId": 8852272761064058498
                },
                {
                    "origLen": 117,
                    "pathId": 5481359131040578315
                }
            ],
            "pathId": -1851346322252275006,
            "blockIdx": 10
        },
        {
            "jpName": "ゼンノロブロイ",
            "enName": "Zenno Rob Roy",
            "jpText": "わ……私だって……成功させたいです！\r\n大好きな物語を知ってもらいたいから――",
            "enText": "I want… to succeed too! I want people to \nknow about my favorite story—",
            "nextBlock": 12,
            "origClipLength": 95,
            "animData": [
                {
                    "origLen": 30,
                    "pathId": -3277311638753855088
                },
                {
                    "origLen": 106,
                    "pathId": 9017475404505056372
                },
                {
                    "origLen": 106,
                    "pathId": -4525280410203550845
                },
                {
                    "origLen": 106,
                    "pathId": 4565603076758321111
                },
                {
                    "origLen": 106,
                    "pathId": 3862880470425365791
                }
            ],
            "pathId": -2233029484599321048,
            "blockIdx": 11
        },
        {
            "jpName": "ゼンノロブロイ",
            "enName": "Zenno Rob Roy",
            "jpText": "ひとかけらの勇気……\r\n私だって出してみせます……！",
            "enText": "A shred of courage… I'll \ngive it to you, too…!",
