                {
                    "origLen": 93,
                    "pathId": -45483654327736590
                }
            ],
            "pathId": 5144836464253702664,
            "blockIdx": 8
        },
        {
            "jpName": "ウマ娘たち",
            "enName": "",
            "jpText": "……ご、ごめんなさ～い！",
            "enText": "",
            "nextBlock": 10,
            "origClipLength": 60,
            "animData": [
                {
                    "origLen": 81,
                    "pathId": -6079020880777750513
