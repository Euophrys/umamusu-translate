        },
        {
            "jpName": "シンボリクリスエス",
            "enName": "",
            "jpText": "！",
            "enText": "",
            "nextBlock": 11,
            "origClipLength": 46,
            "choices": [
                {
                    "jpText": "……驚かせちゃったな",
                    "enText": "",
                    "nextBlock": 11
                },
