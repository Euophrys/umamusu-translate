            "enName": "",
            "jpText": "――用件が――ある。",
            "enText": "",
            "nextBlock": 8,
            "origClipLength": 58,
            "choices": [
                {
                    "jpText": "な……なんだ？",
                    "enText": "",
                    "nextBlock": 8
                },
