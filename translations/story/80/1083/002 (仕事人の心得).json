            "blockIdx": 9
        },
        {
            "jpName": "シンボリクリスエス",
            "enName": "",
            "jpText": "――見覚えは――あるか？",
            "enText": "",
            "nextBlock": 11,
            "origClipLength": 60,
            "choices": [
                {
                    "jpText": "……シャンプー？",
