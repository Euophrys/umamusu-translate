                {
                    "origLen": 109,
                    "pathId": -7537029062978396680
                },
                {
                    "origLen": 71,
                    "pathId": -4145309631101736862
                },
                {
                    "origLen": 126,
                    "pathId": 5069435030481398308
                },
                {
                    "origLen": 109,
                    "pathId": -2247264896604606098
                }
            ],
            "pathId": 793826822831872937,
            "blockIdx": 5
        },
        {
            "jpName": "ハッピーミーク",
            "enName": "Happy Meek",
            "jpText": "…………考えます。",
            "enText": "Think…",
            "nextBlock": 7,
            "origClipLength": 66,
            "animData": [
                {
                    "origLen": 30,
                    "pathId": 449909334894351584
                },
                {
