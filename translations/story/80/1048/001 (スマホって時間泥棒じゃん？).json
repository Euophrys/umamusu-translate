            "pathId": -6887044727645383099,
            "blockIdx": 2
        },
        {
            "jpName": "トーセンジョーダン",
            "enName": "Tosen Jordan",
            "jpText": "め～～～～っちゃ、ダルい……。",
            "enText": "It's ～～～～ so dull…",
            "nextBlock": 4,
            "origClipLength": 64,
            "animData": [
                {
                    "origLen": 41,
                    "pathId": 8695458259107186577
                },
                {
                    "origLen": 75,
                    "pathId": -5827029117005844666
                }
