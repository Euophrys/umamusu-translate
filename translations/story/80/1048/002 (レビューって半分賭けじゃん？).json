            ],
            "pathId": 6129460258828885240,
            "blockIdx": 1
        },
        {
            "jpName": "トーセンジョーダン",
            "enName": "Tosen Jordan",
            "jpText": "はぁ～……。\r\nないわぁ、マジないわぁ……。",
            "enText": "Huh… There's no, there's really no…",
            "nextBlock": 3,
            "origClipLength": 73,
            "choices": [
                {
                    "jpText": "具合が悪そうだな？",
                    "enText": "You don't look well, do you?",
                    "nextBlock": 3
                },
                {
                    "jpText": "具合が悪そうだね？",
                    "enText": "You don't look well, do you?",
                    "nextBlock": 3
                }
            ],
            "animData": [
                {
                    "origLen": 16,
                    "pathId": -8436536018138312259
                },
                {
                    "origLen": 84,
                    "pathId": 1609753838948994681
                }
