                }
            ],
            "pathId": -7286651507429320064,
            "blockIdx": 27
        },
        {
            "jpName": "樫本理子",
            "enName": "Kashimoto Riko",
            "jpText": "ひっ……！？",
            "enText": "What…!",
            "nextBlock": 29,
            "origClipLength": 52,
            "animData": [
                {
                    "origLen": 59,
                    "pathId": -3203066795019427894
                },
