            "pathId": 4508239976643494482,
            "blockIdx": 4
        },
        {
            "jpName": "樫本理子",
            "enName": "Kashimoto Riko",
            "jpText": "あっ、待ちなさい！",
            "enText": "Oh, wait!",
            "nextBlock": 6,
            "origClipLength": 56,
            "animData": [
                {
                    "origLen": 65,
