            "pathId": 3032028436794863895,
            "blockIdx": 22
        },
        {
            "jpName": "メジロドーベル",
            "enName": "Mejiro Dober",
            "jpText": "その……、覚えとく。",
            "enText": "I'll remember that…",
            "nextBlock": -1,
            "origClipLength": 51,
            "trainerGender": "m",
            "animData": [
                {
                    "origLen": 50,
                    "pathId": -7719868052842238314
