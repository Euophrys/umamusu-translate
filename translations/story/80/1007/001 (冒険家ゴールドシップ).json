            "blockIdx": 6
        },
        {
            "jpName": "ゴールドシップ",
            "enName": "Gold Ship",
            "jpText": "――常に心にアドベンチャー！",
            "enText": "—always an adventure in mind!",
            "nextBlock": 8,
            "origClipLength": 63,
