            "blockIdx": 9
        },
        {
            "jpName": "マンハッタンカフェ",
            "enName": "Manhattan Cafe",
            "jpText": "……では。失礼します。",
            "enText": "At… Excuse me.",
            "nextBlock": 11,
            "origClipLength": 64,
            "animData": [
                {
                    "origLen": 102,
                    "pathId": 6191684534738845878
                },
                {
                    "origLen": 102,
