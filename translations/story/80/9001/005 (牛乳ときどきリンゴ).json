            "blockIdx": 7
        },
        {
            "jpName": "駿川たづな",
            "enName": "Hayakawa Tazuna",
            "jpText": "ふふっ、ありがとうございます！\r\nお優しいんですね。",
            "enText": "Phew, thank you so much! You're so kind.",
            "nextBlock": 9,
            "origClipLength": 61,
            "animData": [
                {
                    "origLen": 39,
                    "pathId": 7008894190417485776
                },
                {
                    "origLen": 72,
                    "pathId": -8434557442384034417
                }
