            "blockIdx": 9
        },
        {
            "jpName": "駿川たづな",
            "enName": "Hayakawa Tazuna",
            "jpText": "はぁっ……お待たせしました～！",
            "enText": "Haha… Here you go!",
            "nextBlock": 11,
            "origClipLength": 95,
            "animData": [
                {
                    "origLen": 130,
                    "pathId": 440033830303648703
